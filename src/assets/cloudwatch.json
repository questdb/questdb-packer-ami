{
  "agent": {
    "metrics_collection_interval": 10,
    "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/questdb/logs.log",
            "log_group_name": "/questdb-{instance_id}",
            "log_stream_name": "questdb-{instance_id}",
            "timezone": "UTC"
          }
        ]
      }
    },
    "log_stream_name": "logs",
    "force_flush_interval": 10
  }
}
